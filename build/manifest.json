{
  "package": "com.demo.bottom_tabbar",
  "name": "底部导航栏示例",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "off",
    "designWidth": 375,
    "debug": true
  },
  "router": {
    "entry": "Tabbar",
    "pages": {
      "Tabbar": {
        "component": "tabbar"
      },
      "Index": {
        "component": "index"
      },
      "My": {
        "component": "my"
      },
      "Type": {
        "component": "type"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      },
      "DemoDetail": {
        "titleBarText": "详情页"
      },
      "About": {
        "menu": false
      }
    }
  }
}