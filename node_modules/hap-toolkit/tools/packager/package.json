{
  "name": "loader",
  "version": "0.0.5",
  "subversion": {
    "packager": "0.0.5"
  },
  "description": "a webpack loader for hybrid app",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist/* && rm -rf lib/* && rm -rf test/build/*",
    "build": "npm run lint && babel src --out-dir lib && cp -f package.json lib",
    "release": "rm -rf dist/* && compressfiles lib --out-dir dist --ignore webpack.config.js --copy-files",
    "lint": "eslint src",
    "test": "npm run build && npm run build:test:init && npm run build:test && npm run test:unit",
    "build:test:init": "babel-node test/init.js",
    "test:unit": "mocha --exit test/unit/*.js",
    "build:test": "webpack --config test/webpack.config.js --mode development"
  },
  "dependencies": {
    "@babel/preset-env": "^7.0.0",
    "aaptjs": "^1.3.0",
    "babel-loader": "^8.0.0",
    "babylon": "^6.17.0",
    "babylon-jsx": "^1.0.0",
    "chalk": "^1.1.3",
    "crypto-js": "^3.1.9-1",
    "css": "~2.2.1",
    "dayjs": "^1.7.7",
    "escodegen": "~1.7.1",
    "eslint": "^2.13.1",
    "esprima": "~2.7.0",
    "fs-extra": "^2.0.0",
    "hash-sum": "^1.0.2",
    "jsrsasign": "^7.1.2",
    "jsrsasign-util": "^1.0.0",
    "jszip": "^3.1.5",
    "loader-utils": "~0.2.14",
    "md5": "^2.1.0",
    "parse5": "^3.0.0",
    "prompt": "^1.0.0",
    "source-map": "^0.5.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "compressfiles": "1.0.45",
    "chai": "^4.1.2",
    "css-what": "^2.1.0",
    "file-loader": "^0.9.0",
    "js-base64": "^2.1.9",
    "koa-body": "^2.5.0",
    "koa-router": "^7.2.1",
    "koa-send": "^4.1.1",
    "koa-static": "^4.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "commands": {
    "build": "hap build",
    "release": "hap release",
    "watch": "hap watch"
  }
}
